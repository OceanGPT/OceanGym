{
    "name": "OceanGym",
    "main_agent": "auv0",
    "ticks_per_sec": 100,
    "frames_per_sec": true,
    "octree_min": 0.02,
    "octree_max": 5.0,
    "agents":[
        {
            "agent_name": "auv0",
            "agent_type": "HoveringAUV",
