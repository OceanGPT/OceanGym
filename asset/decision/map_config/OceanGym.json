            "location": [144 ,-114,-63],
            "rotation": [0.0, 0.0, -135.0],
            "sensors": [
                {
                    "sensor_type": "LocationSensor",
                    "socket": "COM",
                    "configuration": {
                        "Sigma": 0
                    }
                },
                {
                    "sensor_type": "RGBCamera",
                    "sensor_name": "FrontCamera",
                    "socket": "CameraSocket",
                    "Hz": 20,
                    "location": [1.0, 0.0, 0.5],
                    "rotation": [0.0, 0.0, 0.0],
                    "configuration": {
                        "CaptureWidth": 512,
                        "CaptureHeight": 512
                    }
                },

                {

                    "sensor_type": "RGBCamera",
                    "sensor_name": "RightCamera",
                    "socket": "CameraLeftSocket",
                    "Hz": 20,
                    "location": [0.0, -1.0, 0.5],
                    "rotation": [0.0, 0.0, -90.0],
                    "configuration": {
                        "CaptureWidth": 256,
                        "CaptureHeight": 256
                    }
                },
                {
                    "sensor_type": "RGBCamera",
                    "sensor_name": "LeftCamera",
                    "socket": "CameraRightSocket",
                    "Hz": 20,
                    "location": [0.0, 1.0, 0.5],
                    "rotation": [0.0, 0.0, 90.0],
                    "configuration": {
                        "CaptureWidth": 256,
                        "CaptureHeight": 256
                    }
                },
                {
                    "sensor_type": "RGBCamera",
                    "sensor_name": "UpCamera",
                    "socket": "CameraSocket",
                    "Hz": 20,
                    "location": [0.0, 0.0, 1.0],
                    "rotation": [0.0, -90.0, 0.0],
                    "configuration": {
                        "CaptureWidth": 256,
                        "CaptureHeight": 256
                    }
                },
                {
                    "sensor_type": "RGBCamera",
                    "sensor_name": "DownCamera",
                    "socket": "CameraSocket",
                    "Hz": 20,
                    "location": [0.0, 0.0, -1.0],
                    "rotation": [0.0, 90.0, 0.0],
                    "configuration": {
                        "CaptureWidth": 256,
                        "CaptureHeight": 256
                    }
                },
                {
                    "sensor_type": "RGBCamera",
                    "sensor_name": "BackCamera",
                    "socket": "CameraSocket",
                    "Hz": 20,
                    "location": [-1.0, 0.0, 0.0],
                    "rotation": [0.0, 0.0, 180.0],
                    "configuration": {
                        "CaptureWidth": 256,
                        "CaptureHeight": 256
                    }
                },
                {
                    "sensor_type": "LocationSensor",
                    "sensor_name": "Location",
                    "socket": "COM",
                    "configuration": {
                        "Sigma": 0
                    }
                }
        
            ]
        }
